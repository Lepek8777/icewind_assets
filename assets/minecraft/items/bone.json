{
  "model": {
      "type": "minecraft:select",
      "property": "minecraft:custom_model_data",
      "cases": [
          {
              "when": "fish_skeleton",
              "model": {
                  "type": "minecraft:model",
                  "model": "icewind:item/fish_skeleton"
              }
          },
          {
              "when": "fish_skeleton_pufferfish",
              "model": {
                  "type": "minecraft:model",
                  "model": "icewind:item/fish_skeleton_pufferfish"
              }
          },
          {
              "when": "seashell",
              "model": {
                  "type": "minecraft:model",
                  "model": "icewind:item/seashell"
              }
          }
      ],
      "fallback": {
          "type": "minecraft:model",
          "model": "minecraft:item/bone"
      }
  }
}