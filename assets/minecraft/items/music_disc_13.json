{
    "model": {
        "type": "minecraft:select",
        "property": "minecraft:custom_model_data",
        "cases": [
            {
                "when": "filled_waterskin",
                "model": {
                    "type": "minecraft:model",
                    "model": "coldsweat_namespace:item/filled_waterskin"
                }
            },
            {
                "when": "waterskin",
                "model": {
                    "type": "minecraft:model",
                    "model": "coldsweat_namespace:item/waterskin"
                }
            },
            {
                "when": "waterskin_cold",
                "model": {
                    "type": "minecraft:model",
                    "model": "coldsweat_namespace:item/waterskin_cold"
                }
            },
            {
                "when": "waterskin_hot",
                "model": {
                    "type": "minecraft:model",
                    "model": "coldsweat_namespace:item/waterskin_hot"
                }
            },
            {
                "when": "hand_fan",
                "model": {
                    "type": "minecraft:model",
                    "model": "coldsweat_namespace:item/hand_fan"
                }
            }
        ],
        "fallback": {
            "type": "minecraft:model",
            "model": "minecraft:item/music_disc_13"
        }
    }
}
